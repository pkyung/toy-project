<!doctype html>
<html lang="ko">
    <head>
        <meta charset="UTF-8">
        <title>friendList</title>
    </head>
    <body>
        <div class="container">

                <table class="table table-hover">
                    <thead>
                        <tr>
                            <th scope="col">친구 아이디</th>
                            <th scope="col">친구 이름</th>
                        </tr>
                    </thead>
                    {{#friends}}
                    <tbody>
                            <tr>
                                <td>{{uuid}}</td>
                                <td>{{name}}</td>
                            </tr>
                    </tbody>
                    {{/friends}}
                </table>
        </div>
    </body>
</html>